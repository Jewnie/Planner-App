{
  "$schema": "https://openapi.vercel.sh/vercel/schemas/config.json",

  "rewrites": [
    { "source": "/api/(.*)", "destination": "/core/src/server.ts" },
    { "source": "/(assets/.*)", "destination": "/dashboard/dist/$1" },
    { "source": "/(vite\\.svg)", "destination": "/dashboard/dist/$1" },
    { "source": "/(.*)", "destination": "/dashboard/dist/index.html" }
  ]
}
