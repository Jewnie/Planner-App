{
  "version": 2,
  "installCommand": "npm install",

  "buildCommand": "npm run build",

  "outputDirectory": "dashboard/dist",

  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/core/src/server.js"
    },
    {
      "source": "/trpc/:path*",
      "destination": "/core/src/server.js"
    }
  ]
}
