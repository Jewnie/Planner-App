{
    "builds": [
      {
        "src": "core/server.ts",
        "use": "@vercel/node"
      },
      {
        "src": "dashboard/package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "core/server.ts" },
      { "src": "/(.*)", "dest": "dashboard/dist/$1" }
    ]
  }