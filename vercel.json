{
    "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
      { "src": "core/src/serverless.ts", "use": "@vercel/node" }
    ],
    "routes": [
    { "src": "/api/(.*)", "dest": "/core/src/serverless.ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
    ]
  }