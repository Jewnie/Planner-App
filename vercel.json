{
    "projects": [
      {
        "name": "dashboard",
        "rootDirectory": "dashboard"
      },
      {
        "name": "core",
        "rootDirectory": "core"
      }
    ],
    "builds": [
      { "src": "dashboard/package.json", "use": "@vercel/next" },
      { "src": "core/api/**/*.ts", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/core/api/$1.ts" },
      { "src": "/(.*)", "dest": "/dashboard/$1" }
    ]
  }
  