{
    "builds": [
      { "src": "dashboard/vite.config.ts", "use": "@vercel/vite" },
      { "src": "core/src/serverless.ts", "use": "@vercel/node" }
    ],
    "routes": [
    { "src": "/api/(.*)", "dest": "/core/src/serverless.ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/dashboard/index.html" }
    ]
  }